{
  "name": "nub-api",
  "private": true,
  "version": "0.0.0",
  "description": "The Nub project API.",
  "keywords": [
    "nub",
    "api",
    "snippets"
  ],
  "dependencies": {
    "async": "2.0.1",
    "bcrypt": "^1.0.3",
    "jsonwebtoken": "^8.0.1",
    "lodash": "3.10.1",
    "mime-types": "^2.1.17",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "sails": "^1.0.0-37",
    "sails-hook-orm": "^2.0.0-16",
    "sails-hook-sockets": "^1.4.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "eslint": "3.5.0",
    "mocha": "^4.0.1"
  },
  "scripts": {
    "start": "node app.js",
    "doc:api": "$(npm bin)/apidoc -i api/controllers -o docs/api",
    "doc": "npm run doc:api",
    "test:unit": "$(npm bin)/mocha test/lifecycle.test.js test/unit/**/*.test.js",
    "test:functional": "$(npm bin)/mocha test/lifecycle.test.js test/functional/**/*.test.js",
    "test": "npm run test:unit && npm run test:functional"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/fwoelffel/nub-api.git"
  },
  "author": {
    "name": "Frédéric Woelffel",
    "email": "hello@fwoelffel.me",
    "url": "https://fwoelffel.me"
  },
  "license": "MIT",
  "engines": {
    "node": "8"
  }
}
